msgid "Animation"
msgstr "Animation"

#: ../../../transition.php:79
msgid "Config"
msgstr "Configuration"

#: ../../../transition.php:82
msgid "Animation duration"
msgstr "Durée de l'animation"

#: ../../../transition.php:84
msgid "Duration of the animation in second. Default : 1"
msgstr "Durée de l'animation en seconde. Défaut : 1"

#: ../../../transition.php:87
msgid "Animation delay"
msgstr "Delai avant animation"

#: ../../../transition.php:89
msgid "Delay before the animation start in second. Default : 0"
msgstr "Delai avant que l'animation ne commence, en seconde. Défaut : 0"

#: ../../../transition.php:92
msgid "Parallax image height"
msgstr "Hauteur de l'image en parallax"

#: ../../../transition.php:94
msgid ""
"Height ratio of the parallax image relative to the height of the screen. "
"Default : 0.4"
msgstr "Hauteur en ration par rapport à la taille de l'écran. Défaut : 0.4"

